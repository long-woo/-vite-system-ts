{
  "name": "vite-system-ts",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "bun build ./src/index.ts --outfile ./dist/vite-system-ts.js --target browser --format esm --minify --external '*'",
    "build:esbuild": "bun ./scripts/build.js",
    "format": "bunx @biomejs/biome format --write ./src",
    "lint": "bunx @biomejs/biome lint ./src",
    "check": "bunx @biomejs/biome check --apply ./src"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.3",
    "@types/systemjs": "^6.13.4",
    "bun-types": "latest",
    "esbuild": "^0.19.5",
    "systemjs": "^6.14.2",
    "tslib": "^2.6.2",
    "vite": "^4.5.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@rollup/browser": "^4.2.0"
  }
}